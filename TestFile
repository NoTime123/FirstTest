#include <iostream>
#include <cstring>
#include <array>
#include <vector>
int main()
{
	using namespace std;
	array<int, 4> a = { 1,2,3,4 };
	array<int, 4> b;
	b = a;
	vector<int>c(4);
	c[0] = 0;
	cout << c[0] << endl;
	cout << a[1] << endl << b[1] << endl;
	//a.at(-1) = 2;
	//a[-1] = 2;
	return 0;
}
